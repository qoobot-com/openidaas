      "description": "JWT 配置"
    },
    {
      "name": "openidaas.security",
      "type": "com.qoobot.openidaas.starter.properties.OpenIDaaSProperties$SecurityProperties",
      "sourceType": "com.qoobot.openidaas.starter.properties.OpenIDaaSProperties",
      "description": "安全配置"
    },
    {
      "name": "openidaas.security.password-policy",
      "type": "com.qoobot.openidaas.starter.properties.OpenIDaaSProperties$PasswordPolicyProperties",
